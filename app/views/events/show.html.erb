<p id="notice"><%= notice %></p>

<p>
  <b>Start:</b>
  <%= @event.start %>
</p>

<p>
  <b>End:</b>
  <%= @event.end %>
</p>

<p>
  <b>Location:</b>
  <%= @event.location.name %>
</p>

 <p>
    <b>Check-ins:</b>
    <% @checkins.each do |checkin| %>
      <ul>
        <%= link_to image_tag(checkin.user.avatar.url(:thumb)), user_path(checkin.user) %>
      </ul>
    <% end %>
 </p> 

<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %>
